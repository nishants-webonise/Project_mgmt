<h1>Sign in</h1>

<%= form_for @user_session, :url => user_sessions_path do |f| %>

    <%= render "shared/error_messages", :target => @user_session %>

    <%= f.label :username %>
    <%= f.text_field :username %><br />

    <%= f.label :password %>
    <%= f.password_field :password %><br />

    <%= f.submit "SIGN IN" %>

<% end %>
